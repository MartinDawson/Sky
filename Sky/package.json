  "scripts": {
    "prestart": "webpack",
    "setup": "run-s restore-nuget",
    "restore-nuget": "dotnet restore",
    "development": "cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "relay-compiler": "relay-compiler --src ./Components --schema schema.graphql --language typescript --artifactDirectory ./Components/__generated__ --extensions tsx",
    "relay-compiler:watch": "npm run relay-compiler -- --watch",
    "get-schema": "graphql get-schema --project app",
    "lint": "tslint --fix ./Components/**/*.tsx",
    "clean": "rimraf ./wwwroot/build",
    "build-client": "webpack",
    "build": "run-s clean build-client"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|integration))\\.tsx?$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./Components/testHelper/index.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "browserslist": [
    "> 3%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "edge >= 12",
    "Explorer 11"
  ],
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/enzyme": "^3.1.11",
